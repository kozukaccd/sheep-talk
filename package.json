{
  "name": "sheep-talk",
  "version": "0.1.0",
  "author": "iChiba",
  "description": "speech recognition for streamers.",
  "scripts": {
    "dev": "set MODE=development&&vite",
    "build": "vite build",
    "electron-forge:build": "set MODE=production && vite build && electron-forge import && electron-forge make",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "main": "dist/electron/main/index.js",
  "dependencies": {
    "@google-cloud/speech": "^4.10.2",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
    "@mojs/core": "^1.3.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "rc-slider": "^10.0.0",
    "react": "^18.0.0",
    "react-burger-menu": "^3.0.8",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^4.2.21",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "svg-parser": "^2.0.4",
    "vite-svg-loader": "^3.3.0",
    "voice-activity-detection": "0.0.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "electron": "^19.0.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "npm-run-all": "^4.1.5",
    "vite": "^2.9.9",
    "vite-plugin-electron": "^0.4.8"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3000
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sheep_talk"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
